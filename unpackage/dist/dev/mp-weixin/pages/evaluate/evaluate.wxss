.main .evaluate .header {
  padding: 20rpx 0;
  -webkit-justify-content: space-around;
          justify-content: space-around;
}
.main .evaluate .header .left {
  font-size: 60rpx;
  color: #EE7707;
  padding: 20rpx;
}
.main .evaluate .header .left .show {
  font-size: 24rpx;
  color: #c1c1c1;
}
.main .evaluate .header .icon view {
  padding: 10rpx;
}
.main .evaluate .header .icon .vote-star {
  height: 50rpx;
  width: 300rpx;
  background: url(data:image/png;base64,) 0 15rpx/30rpx 30rpx repeat-x;
}
.main .evaluate .header .icon .vote-star .i {
  height: 50rpx;
  background: url(data:image/png;base64,) 0 15rpx/30rpx 30rpx repeat-x;
}
.main .evaluate .header .pai {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.main .evaluate .header .pai image {
  width: 100rpx;
  height: 100rpx;
}
.main .evaluate .header .pai .font-lg {
  font-size: 32rpx;
}
.main .category {
  border-top: 1rpx solid #e3e1e8;
  border-bottom: 1rpx solid #e3e1e8;
  padding: 10rpx;
  width: 100%;
  white-space: pre-wrap;
  font-size: 24rpx;
}
.main .category .select-tag {
  display: inline-block;
  padding: 10rpx 20rpx;
  margin: 10rpx 10rpx;
  color: #fff;
  white-space: nowrap;
}
.main .category .light {
  color: #000;
}
.main .category .dark {
  color: #000;
  position: relative;
}
.main .category .dark::after {
  content: '';
  width: 80rpx;
  position: absolute;
  bottom: -20rpx;
  left: 0;
  right: 0;
  margin: auto;
  border-bottom: 4rpx solid #fa3534;
}
.main .people {
  padding-bottom: 40rpx;
}
.main .people .once {
  padding: 40rpx 40rpx 0rpx 40rpx;
}
.main .people .once .line {
  margin-top: 20rpx;
  padding: 0;
}
.main .people .once .top .title {
  margin-left: 20rpx;
  font-weight: 700;
  font-size: 28rpx;
}
.main .people .once .detail {
  font-size: 24rpx;
  color: #c0c0c0;
  margin-bottom: 20rpx;
}
.main .people .once .others {
  -webkit-justify-content: space-around;
          justify-content: space-around;
  padding: 10rpx 0;
  border: 1px solid #ddd;
  margin-bottom: 30rpx;
}
.main .people .once .others .box {
  text-align: center;
  margin: 0 5rpx;
  color: #EE7707;
  font-size: 32rpx;
}
.main .people .once .others .box .text {
  white-space: nowrap;
  color: #000;
  font-size: 24rpx;
}
.main .people .once .others .l-height {
  position: relative;
}
.main .people .once .content .text {
  font-size: 32rpx;
  margin-bottom: 10rpx;
}
.main .people .once .content .imgbox {
  -webkit-flex-flow: wrap;
          flex-flow: wrap;
}
.main .people .once .content .imgbox .img {
  width: 200rpx;
  height: 200rpx;
  margin: 10rpx;
}
.main .people .more {
  padding: 60rpx 0;
}

