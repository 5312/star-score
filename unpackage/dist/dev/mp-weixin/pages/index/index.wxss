.listview {
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  flex-direction: column;
}
.wrap .group {
  width: auto !important;
  margin: 20rpx;
  padding: 25rpx;
  color: #606266;
  background-color: #fff;
  border-radius: 16rpx;
}
.wrap .group .left .right {
  text-align: right;
  white-space: nowrap;
  width: 100rpx;
}
.wrap .group .left .right .icon {
  margin-bottom: 24rpx;
  -webkit-transform: rotate(90deg);
          transform: rotate(90deg);
}
.wrap .group .left .right .height {
  height: 70rpx;
  line-height: 70rpx;
  font-size: 28rpx;
}
.wrap .group .left .title {
  margin-left: 20rpx;
}
.wrap .group .left .title .detail {
  white-space: nowrap;
  width: 300rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 24rpx;
  color: #c0c0c0;
  font-weight: 400;
}
.wrap .group .left .title .shop_name {
  white-space: nowrap;
  width: 350rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  font-weight: 600;
  font-size: 32rpx;
  color: #333;
  margin-bottom: 20rpx;
}
.wrap .group .left .title .number {
  color: #ee6300;
  font-weight: 600;
}
.wrap .group .left .title .number .image {
  width: 80rpx;
  height: 80rpx;
}
.wrap .group .left .img {
  width: 180rpx;
  height: 180rpx;
  border-radius: 10rpx;
}

